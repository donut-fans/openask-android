<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<!-- 底部绿色按钮 -->
<item android:left="@dimen/sw_6dp" android:top="@dimen/sw_6dp">
    <shape android:shape="rectangle">
        <solid android:color="#CAFE07" />
        <corners android:radius="@dimen/sw_6dp"/>
        <stroke android:width="@dimen/sw_2dp" android:color="@color/black"/>
    </shape>
</item>
<!-- 上部白色按钮 -->
<item android:bottom="@dimen/sw_6dp" android:right="@dimen/sw_6dp">
    <shape android:shape="rectangle">
        <solid android:color="#FFFFFF" />
        <corners android:radius="@dimen/sw_6dp"/>
        <stroke android:width="@dimen/sw_2dp" android:color="@color/black"/>
    </shape>
</item>
</layer-list>